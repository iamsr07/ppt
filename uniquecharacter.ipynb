class Solution:
    def firstUniqChar(self, s: str) -> int:

      count = {}
        
        n = len(s)
      
        for i in range(n):
            c = s[i]
            count[c] = count.get(c, 0) + 1

        for i in range(n):
            if count[s[i]] == 1:
                return i
        return -1
